{
  "cSpell.words": [
    "attributify",
    "codemirror",
    "composables",
    "demi",
    "docsearch",
    "hljs",
    "prismjs",
    "splitpanes"
  ],
  "eslint.packageManager": "pnpm",
  "prettier.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "volar.takeOverBuiltinTsExtension": true,
